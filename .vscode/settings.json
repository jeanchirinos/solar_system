{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**.exe": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true,
    "node_modules": true,
    ".gitignore": true,
    ".prettierrc.mjs": true,
    "README.md": true
  },
  "hide-files.files": [
    "node_modules",
    ".gitignore",
    ".prettierrc.mjs",
    "README.md"
  ],
  "prettier.documentSelectors": [
    "**/*.astro"
  ],
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}